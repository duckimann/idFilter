{
    "manifest_version": 2,
    "name": "idFilter",
    "version": "1.0",
    "browser_action": {
        "default_title": "ID Filter"
    },
    "background": {
        "scripts": ["bg.js"],
        "persistence": false
    },
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "tabs",
        "webRequest",
        "<all_urls>"
    ]
}